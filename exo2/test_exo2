# Unit tests
fixed_tests_True = (
    ( "samurai", "ai"    ),
    ( "ninja",   "ja"    ),
    ( "sensei",  "i"     ),
    ( "abc",     "abc"   ),
    ( "abcabc",  "bc"    ),
    ( "fails",   "ails"  ),
)

fixed_tests_False = (
    ( "sumo",    "omo"   ),
    ( "samurai", "ra"    ),
    ( "abc",     "abcd"  ),
    ( "ails",    "fails" ),
    ( "this",    "fails" ),
    ( "spam",    "eggs"  )
)

for test in fixed_tests_True:
    input_str, ending_str = test
    result = solution(input_str, ending_str)
    print(f"Input: {test}, Result: {result}, Expected: True")

for test in fixed_tests_False:
    input_str, ending_str = test
    result = solution(input_str, ending_str)
    print(f"Input: {test}, Result: {result}, Expected: False")
